extends layout

block content
  h1= title
  -let results =["public\images\blackvan.jpg","public\images\redvan.jpg","public\images\van.jpg","public\images\van1.jpg","public\images\whitevan.jpg"]
  -for(var i=0;i<3; i++) {
    -let rand = Math.floor(Math.random()*5)
    img(src=`images/${results[rand]}`,height="300px",width="300px")
  -}